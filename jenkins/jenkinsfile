pipeline{
    agent any
    stages{
        stage('Build'){
            step{
                sh'''
                cat /etc/os-relese
                '''
            }
               
        }
        stage('build'){
            steps{
                sh'''
                echo"memory details: " free -m
                '''
            }
        }
        stage('build-cpu-details)'{
            steps{
                sh'''
                echo"cpu details: " 
                lscpu
                '''
            }
        }
        stage('build-ip){
            steps{
                sh'''
                echo"ip details: " 
                hostname -I
                '''
            }
        }
        stage('build-cal){
            steps{
                sh'''
                echo"cal details: " 
                cal
                '''
            }
        }
    }
    
}
